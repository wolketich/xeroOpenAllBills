{
    "manifest_version": 3,
    "name": "Open All Links",
    "version": "1.0",
    "description": "Opens all links from a specific HTML structure.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }